<AnchorPane prefHeight="691.0" prefWidth="1346.0" styleClass="displayPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.guiControls.adminMain.assetsController.AssetsController">
   <children>
      <ScrollPane hbarPolicy="NEVER" layoutY="150.0" prefHeight="481.0" prefWidth="1300.0" styleClass="bodyPane">
         <content>
            <VBox fx:id="assetsDisplayBox" prefWidth="1362.0" spacing="20.0" />
         </content>
      </ScrollPane>
      <HBox alignment="CENTER" layoutX="80.0" layoutY="80.0" prefHeight="60.0" prefWidth="1300.0" spacing="20.0" style="-fx-background-color: 000000;">
         <children>
            <Label alignment="CENTER" prefHeight="60.0" prefWidth="33.0" styleClass="tableHeadlines" text="ID" />
            <Label alignment="CENTER" prefHeight="60.0" prefWidth="137.0" styleClass="tableHeadlines" text="Name" />
            <Label alignment="CENTER" prefHeight="60.0" prefWidth="556.0" styleClass="tableHeadlines" text="Description" />
            <Label alignment="CENTER" prefHeight="80.0" prefWidth="100.0" />
            <Label alignment="CENTER" prefHeight="80.0" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="5.0" prefHeight="80.0" prefWidth="1200.0" spacing="20.0" style="-fx-background-radius: 20;" styleClass="whitePane">
         <children>
            <Button fx:id="refreshAssetsButton" mnemonicParsing="false" onAction="#update" prefHeight="31.0" prefWidth="153.0" text="Refresh" />
            <TextField fx:id="newNameTextField" prefHeight="30.0" prefWidth="300.0" promptText="Name">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
            <TextField fx:id="newDescriptionTextField" prefHeight="30.0" prefWidth="700.0" promptText="Description">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
            <Button fx:id="newAssetButton" mnemonicParsing="false" onAction="#addEntry" prefHeight="48.0" prefWidth="146.0" text="Add">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
